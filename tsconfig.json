{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "target": "ES6",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["app/components/*"],
      "@/actions/*": ["app/actions/*"],
      "@/api/*": ["app/api/*"],
      "@/utils/*": ["app/utils/*"],
      "@/types/*": ["types/*"],
      "@/models/*": ["models/*"],
      "@/helpers/*": ["app/helpers/*"],
      "@/hooks/*": ["app/hooks/*"],
      "@/config/*": ["config/*"],
      "@/lib/*": ["lib/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "auth.tjs", "auth.js"],
  "exclude": ["node_modules"]
}

{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "target": "ES6",
    "plugins": [
      {
        "organize imports": "prettier-plugin-organize-imports"
      },
      {
        "name": "next"
      }
    ],
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@/app/*": ["./app/*"],
      "@/context/*": ["./app/context/*"],
      "@/components/*": ["./app/components/*"],
      "@/components": ["./app/components/index"],
      "@/actions/*": ["./app/actions/*"],
      "@/api/*": ["./app/api/*"],
      "@/utils/*": ["./app/utils/*"],
      "@/types/*": ["./types/*"],
      "@/models/*": ["./models/*"],
      "@/models/": ["./models/index"],
      "@/helpers/*": ["./app/helpers/*"],
      "@/helpers/": ["./app/helpers/index*"],
      "@/hooks/*": ["./app/hooks/*"],
      "@/hooks/": ["./app/hooks/index"],
      "@/config/*": ["./app/config/*"],
      "@/lib/*": ["./app/lib/*"],
      "@/lib": ["./app/lib/index"],
      "@/admin/*": ["app/(admin)/*"]
    },
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "auth.js",
    "auth.tjs",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "app/helpers/server/jwt.ts."
  ],
  "exclude": ["node_modules"]
}
